{
  "name": "ReactNativeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky",
    "web": "webpack serve --mode development",
    "build:web": "cross-env NODE_ENV=production webpack --mode production",
    "deploy": "gh-pages -d dist",
    "preview": "npx webpack --mode production&npx serve dist"
  },
  "dependencies": {
    "@react-native-vector-icons/evil-icons": "^12.4.0",
    "@react-native-vector-icons/fontawesome6": "^12.3.0",
    "@react-native/new-app-screen": "0.80.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-native": "0.80.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-loader": "^10.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.19.0",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.2.6",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
